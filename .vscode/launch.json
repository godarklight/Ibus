    "version": "0.2.0",
    "configurations": [
        {
            "name": "Serial",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/net5/bin/Debug/net5.0/Ibus.dll",
            "args": ["serial"],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "TCPIO",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/net5/bin/Debug/net5.0/Ibus.dll",
            "args": [
                "tcp"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "FileIO",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/net5/bin/Debug/net5.0/Ibus.dll",
            "args": [
                "file"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
    ]
}